{
    "manifest_version": 2.0,
    "id": "gridtechgeomap",
    "name": "Gridtech GeoMap",
    "version": "1.0",
    "type": "module",
    "namespace": "GridtechGeoMap",
    "author": "Gustavo Zeloni",
    "url": "https://github.com/GridTechRnD/zabbix_interactive_map/tree/zabbix_module",
    "description": "Displays an interactive geographic map highlighting hosts and clusters for better visualization and management.",
    "actions": {
        "gridtechgeomaplist.view": {
            "class": "GridtechGeoMapList",
            "view": "gridtechgeomaplist.view"
        },
        "gridtechgeomap.view": {
            "class": "GridtechGeoMapHosts",
            "view": "gridtechgeomap.view"
        },
        "executescript.action": {
            "class": "ExecuteScript"
        }
    }
}
